                "scrollbar.track.border": "#211012",
                "scrollbar.thumb.background": "#987491",
                "editor.gutter.background": "#21101290",
                "terminal.background": "#21101210",
                "terminal.foreground": "#c7c3c3",
                "terminal.dim_foreground": "#c7c3c3",
                "terminal.bright_foreground": "#c7c3c3",
                "terminal.ansi.black": "#211012",
                "terminal.ansi.red": "#987491",
                "terminal.ansi.green": "#DB768A",
                "terminal.ansi.yellow": "#B08DA3",
                "terminal.ansi.blue": "#D79CAD",
                "terminal.ansi.magenta": "#A4B3E7",
                "terminal.ansi.cyan": "#E6B0CB",
                "terminal.ansi.white": "#c7c3c3",
                "terminal.ansi.bright_black": "#725e5f",
                "terminal.ansi.bright_red": "#987491",
                "terminal.ansi.bright_green": "#DB768A",
                "terminal.ansi.bright_yellow": "#B08DA3",
