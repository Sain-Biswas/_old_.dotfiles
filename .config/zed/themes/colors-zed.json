{
    "$schema": "https://zed.dev/schema/themes/v0.1.0.json",
    "name": "pywal",
    "author": "pywal",
    "themes": [
        {
            "name": "pywal",
            "appearance": "dark",
            "style": {
                "background": "#38191e90",
                "editor.background": "#38191e90",
                "editor.foreground": "#cdc5c6",
                "text": "#cdc5c6",
                "text.muted": "#cdc5c670",
                "text.ignored": "#cdc5c640",
                "text.placeholder": "#cdc5c650",
                "ignored": "#cdc5c630",
                "element.hover": "#cdc5c630",
                "ghost_element.hover": "#D08B9330",
                "ghost_element.selected": "#D08B9330",
                "ghost_element.active": "#D08B9360",
                "border": "#38191e",
                "editor.highlighted_line.background": "#D08B9310",
                "editor.active_line.background": "#D08B9310",
                "panel.background": "#38191e90",
                "title_bar.background": "#38191e90",
                "title_bar.inactive_background": "#38191e90",
                "status_bar.background": "#38191e90",
                "drop_target.background": "#38191e90",
                "elevated_surface.background": "#38191e",
                "toolbar.background": "#38191e90",
                "tab_bar.background": "#38191e90",
                "tab.inactive_background": "#38191e90",
                "tab.active_background": "#D08B9330",
                "scrollbar.track.background": "transparent",
                "scrollbar.track.border": "#38191e",
                "scrollbar.thumb.background": "#D08B93",
                "editor.gutter.background": "#38191e90",
                "terminal.background": "#38191e10",
                "terminal.foreground": "#cdc5c6",
                "terminal.dim_foreground": "#cdc5c6",
                "terminal.bright_foreground": "#cdc5c6",
                "terminal.ansi.black": "#38191e",
                "terminal.ansi.red": "#D08B93",
                "terminal.ansi.green": "#CFAAB1",
                "terminal.ansi.yellow": "#E0A4A7",
                "terminal.ansi.blue": "#E8C3BA",
                "terminal.ansi.magenta": "#D6BAC5",
                "terminal.ansi.cyan": "#F0CFD2",
                "terminal.ansi.white": "#cdc5c6",
                "terminal.ansi.bright_black": "#7e676a",
                "terminal.ansi.bright_red": "#D08B93",
                "terminal.ansi.bright_green": "#CFAAB1",
                "terminal.ansi.bright_yellow": "#E0A4A7",
                "terminal.ansi.bright_blue": "#E8C3BA",
                "terminal.ansi.bright_magenta": "#D6BAC5",
                "terminal.ansi.bright_cyan": "#F0CFD2",
                "terminal.ansi.bright_white": "#cdc5c6",
                "modified": "#D08B93",
                "syntax": {
                    "attribute": {
                        "color": "#D08B93"
                    },
                    "boolean": {
                      "color": "#CFAAB1"
                    },
                    "comment": {
                      "color": "#7e676a"
                    },
                    "comment.doc": {
                      "color": "#7e676a"
                    },
                    "constant": {
                      "color": "#CFAAB1"
                    },
                    "function": {
                      "color": "#F0CFD2"
                    },
                    "keyword": {
                      "color": "#E8C3BA"
                    },
                    "number": {
                      "color": "#D6BAC5"
                    },
                    "operator": {
                      "color": "#E8C3BA"
                    },
                    "string": {
                      "color": "#D08B93"
                    },
                    "variable": {
                      "color": "#CFAAB1"
                    }
                },
                "players": [
                  {
                    "cursor": "#D08B93",
                    "background": "#38191e",
                    "selection": "#D08B9330"
                  }
                ]
            }
        }
    ]
}
