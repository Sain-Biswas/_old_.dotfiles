                "editor.foreground": "#c7c3c3",
                "text": "#c7c3c3",
                "text.muted": "#c7c3c370",
                "text.ignored": "#c7c3c340",
                "text.placeholder": "#c7c3c350",
                "ignored": "#c7c3c330",
                "element.hover": "#c7c3c330",
                "ghost_element.hover": "#98749130",
                "ghost_element.selected": "#98749130",
                "ghost_element.active": "#98749160",
                "border": "#211012",
                "editor.highlighted_line.background": "#98749110",
                "editor.active_line.background": "#98749110",
                "panel.background": "#21101290",
                "title_bar.background": "#21101290",
                "title_bar.inactive_background": "#21101290",
                "status_bar.background": "#21101290",
                "drop_target.background": "#21101290",
                "elevated_surface.background": "#211012",
                "toolbar.background": "#21101290",
                "tab_bar.background": "#21101290",
                "tab.inactive_background": "#21101290",
                "tab.active_background": "#98749130",
