{
    "$schema": "https://zed.dev/schema/themes/v0.1.0.json",
    "name": "pywal",
    "author": "pywal",
    "themes": [
        {
            "name": "pywal",
            "appearance": "dark",
            "style": {
                "background": "#10121690",
                "editor.background": "#10121690",
                "editor.foreground": "#c3c3c4",
                "text": "#c3c3c4",
                "text.muted": "#c3c3c470",
                "text.ignored": "#c3c3c440",
                "text.placeholder": "#c3c3c450",
                "ignored": "#c3c3c430",
                "element.hover": "#c3c3c430",
                "ghost_element.hover": "#5F5E7130",
                "ghost_element.selected": "#5F5E7130",
                "ghost_element.active": "#5F5E7160",
                "border": "#101216",
                "editor.highlighted_line.background": "#5F5E7110",
                "editor.active_line.background": "#5F5E7110",
                "panel.background": "#10121690",
                "title_bar.background": "#10121690",
                "title_bar.inactive_background": "#10121690",
                "status_bar.background": "#10121690",
                "drop_target.background": "#10121690",
                "elevated_surface.background": "#101216",
                "toolbar.background": "#10121690",
                "tab_bar.background": "#10121690",
                "tab.inactive_background": "#10121690",
                "tab.active_background": "#5F5E7130",
                "scrollbar.track.background": "transparent",
                "scrollbar.track.border": "#101216",
                "scrollbar.thumb.background": "#5F5E71",
                "editor.gutter.background": "#10121690",
                "terminal.background": "#10121610",
                "terminal.foreground": "#c3c3c4",
                "terminal.dim_foreground": "#c3c3c4",
                "terminal.bright_foreground": "#c3c3c4",
                "terminal.ansi.black": "#101216",
                "terminal.ansi.red": "#5F5E71",
                "terminal.ansi.green": "#696A79",
                "terminal.ansi.yellow": "#757386",
                "terminal.ansi.blue": "#7E7D87",
                "terminal.ansi.magenta": "#897892",
                "terminal.ansi.cyan": "#9F97A0",
                "terminal.ansi.white": "#c3c3c4",
                "terminal.ansi.bright_black": "#5a5f6f",
                "terminal.ansi.bright_red": "#5F5E71",
                "terminal.ansi.bright_green": "#696A79",
                "terminal.ansi.bright_yellow": "#757386",
                "terminal.ansi.bright_blue": "#7E7D87",
                "terminal.ansi.bright_magenta": "#897892",
                "terminal.ansi.bright_cyan": "#9F97A0",
                "terminal.ansi.bright_white": "#c3c3c4",
                "modified": "#5F5E71",
                "syntax": {
                    "attribute": {
                        "color": "#5F5E71"
                    },
                    "boolean": {
                      "color": "#696A79"
                    },
                    "comment": {
                      "color": "#5a5f6f"
                    },
                    "comment.doc": {
                      "color": "#5a5f6f"
                    },
                    "constant": {
                      "color": "#696A79"
                    },
                    "function": {
                      "color": "#9F97A0"
                    },
                    "keyword": {
                      "color": "#7E7D87"
                    },
                    "number": {
                      "color": "#897892"
                    },
                    "operator": {
                      "color": "#7E7D87"
                    },
                    "string": {
                      "color": "#5F5E71"
                    },
                    "variable": {
                      "color": "#696A79"
                    }
                },
                "players": [
                  {
                    "cursor": "#5F5E71",
                    "background": "#101216",
                    "selection": "#5F5E7130"
                  }
                ]
            }
        }
    ]
}
