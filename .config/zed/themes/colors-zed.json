{
    "$schema": "https://zed.dev/schema/themes/v0.1.0.json",
    "name": "pywal",
    "author": "pywal",
    "themes": [
        {
            "name": "pywal",
            "appearance": "dark",
            "style": {
                "background": "#0a113490",
                "editor.background": "#0a113490",
                "editor.foreground": "#c1c3cc",
                "text": "#c1c3cc",
                "text.muted": "#c1c3cc70",
                "text.ignored": "#c1c3cc40",
                "text.placeholder": "#c1c3cc50",
                "ignored": "#c1c3cc30",
                "element.hover": "#c1c3cc30",
                "ghost_element.hover": "#2C4DC630",
                "ghost_element.selected": "#2C4DC630",
                "ghost_element.active": "#2C4DC660",
                "border": "#0a1134",
                "editor.highlighted_line.background": "#2C4DC610",
                "editor.active_line.background": "#2C4DC610",
                "panel.background": "#0a113490",
                "title_bar.background": "#0a113490",
                "title_bar.inactive_background": "#0a113490",
                "status_bar.background": "#0a113490",
                "drop_target.background": "#0a113490",
                "elevated_surface.background": "#0a1134",
                "toolbar.background": "#0a113490",
                "tab_bar.background": "#0a113490",
                "tab.inactive_background": "#0a113490",
                "tab.active_background": "#2C4DC630",
                "scrollbar.track.background": "transparent",
                "scrollbar.track.border": "#0a1134",
                "scrollbar.thumb.background": "#2C4DC6",
                "editor.gutter.background": "#0a113490",
                "terminal.background": "#0a113410",
                "terminal.foreground": "#c1c3cc",
                "terminal.dim_foreground": "#c1c3cc",
                "terminal.bright_foreground": "#c1c3cc",
                "terminal.ansi.black": "#0a1134",
                "terminal.ansi.red": "#2C4DC6",
                "terminal.ansi.green": "#0D6BCC",
                "terminal.ansi.yellow": "#2067C9",
                "terminal.ansi.blue": "#4EA8BC",
                "terminal.ansi.magenta": "#35BACC",
                "terminal.ansi.cyan": "#609CCE",
                "terminal.ansi.white": "#c1c3cc",
                "terminal.ansi.bright_black": "#626577",
                "terminal.ansi.bright_red": "#2C4DC6",
                "terminal.ansi.bright_green": "#0D6BCC",
                "terminal.ansi.bright_yellow": "#2067C9",
                "terminal.ansi.bright_blue": "#4EA8BC",
                "terminal.ansi.bright_magenta": "#35BACC",
                "terminal.ansi.bright_cyan": "#609CCE",
                "terminal.ansi.bright_white": "#c1c3cc",
                "modified": "#2C4DC6",
                "syntax": {
                    "attribute": {
                        "color": "#2C4DC6"
                    },
                    "boolean": {
                      "color": "#0D6BCC"
                    },
                    "comment": {
                      "color": "#626577"
                    },
                    "comment.doc": {
                      "color": "#626577"
                    },
                    "constant": {
                      "color": "#0D6BCC"
                    },
                    "function": {
                      "color": "#609CCE"
                    },
                    "keyword": {
                      "color": "#4EA8BC"
                    },
                    "number": {
                      "color": "#35BACC"
                    },
                    "operator": {
                      "color": "#4EA8BC"
                    },
                    "string": {
                      "color": "#2C4DC6"
                    },
                    "variable": {
                      "color": "#0D6BCC"
                    }
                },
                "players": [
                  {
                    "cursor": "#2C4DC6",
                    "background": "#0a1134",
                    "selection": "#2C4DC630"
                  }
                ]
            }
        }
    ]
}
