{
    "$schema": "https://zed.dev/schema/themes/v0.1.0.json",
    "name": "pywal",
    "author": "pywal",
    "themes": [
        {
            "name": "pywal",
            "appearance": "dark",
            "style": {
                "background": "#12151890",
                "editor.background": "#12151890",
                "editor.foreground": "#c3c4c5",
                "text": "#c3c4c5",
                "text.muted": "#c3c4c570",
                "text.ignored": "#c3c4c540",
                "text.placeholder": "#c3c4c550",
                "ignored": "#c3c4c530",
                "element.hover": "#c3c4c530",
                "ghost_element.hover": "#B2A35C30",
                "ghost_element.selected": "#B2A35C30",
                "ghost_element.active": "#B2A35C60",
                "border": "#121518",
                "editor.highlighted_line.background": "#B2A35C10",
                "editor.active_line.background": "#B2A35C10",
                "panel.background": "#12151890",
                "title_bar.background": "#12151890",
                "title_bar.inactive_background": "#12151890",
                "status_bar.background": "#12151890",
                "drop_target.background": "#12151890",
                "elevated_surface.background": "#121518",
                "toolbar.background": "#12151890",
                "tab_bar.background": "#12151890",
                "tab.inactive_background": "#12151890",
                "tab.active_background": "#B2A35C30",
                "scrollbar.track.background": "transparent",
                "scrollbar.track.border": "#121518",
                "scrollbar.thumb.background": "#B2A35C",
                "editor.gutter.background": "#12151890",
                "terminal.background": "#12151810",
                "terminal.foreground": "#c3c4c5",
                "terminal.dim_foreground": "#c3c4c5",
                "terminal.bright_foreground": "#c3c4c5",
                "terminal.ansi.black": "#121518",
                "terminal.ansi.red": "#B2A35C",
                "terminal.ansi.green": "#5D7083",
                "terminal.ansi.yellow": "#B1739C",
                "terminal.ansi.blue": "#68B6C5",
                "terminal.ansi.magenta": "#DA96AF",
                "terminal.ansi.cyan": "#C5A5BC",
                "terminal.ansi.white": "#c3c4c5",
                "terminal.ansi.bright_black": "#5b6670",
                "terminal.ansi.bright_red": "#B2A35C",
                "terminal.ansi.bright_green": "#5D7083",
                "terminal.ansi.bright_yellow": "#B1739C",
                "terminal.ansi.bright_blue": "#68B6C5",
                "terminal.ansi.bright_magenta": "#DA96AF",
                "terminal.ansi.bright_cyan": "#C5A5BC",
                "terminal.ansi.bright_white": "#c3c4c5",
                "modified": "#B2A35C",
                "syntax": {
                    "attribute": {
                        "color": "#B2A35C"
                    },
                    "boolean": {
                      "color": "#5D7083"
                    },
                    "comment": {
                      "color": "#5b6670"
                    },
                    "comment.doc": {
                      "color": "#5b6670"
                    },
                    "constant": {
                      "color": "#5D7083"
                    },
                    "function": {
                      "color": "#C5A5BC"
                    },
                    "keyword": {
                      "color": "#68B6C5"
                    },
                    "number": {
                      "color": "#DA96AF"
                    },
                    "operator": {
                      "color": "#68B6C5"
                    },
                    "string": {
                      "color": "#B2A35C"
                    },
                    "variable": {
                      "color": "#5D7083"
                    }
                },
                "players": [
                  {
                    "cursor": "#B2A35C",
                    "background": "#121518",
                    "selection": "#B2A35C30"
                  }
                ]
            }
        }
    ]
}
