{
    "$schema": "https://zed.dev/schema/themes/v0.1.0.json",
    "name": "pywal",
    "author": "pywal",
    "themes": [
        {
            "name": "pywal",
            "appearance": "dark",
            "style": {
                "background": "#19143d90",
                "editor.background": "#19143d90",
                "editor.foreground": "#c5c4ce",
                "text": "#c5c4ce",
                "text.muted": "#c5c4ce70",
                "text.ignored": "#c5c4ce40",
                "text.placeholder": "#c5c4ce50",
                "ignored": "#c5c4ce30",
                "element.hover": "#c5c4ce30",
                "ghost_element.hover": "#F2629E30",
                "ghost_element.selected": "#F2629E30",
                "ghost_element.active": "#F2629E60",
                "border": "#19143d",
                "editor.highlighted_line.background": "#F2629E10",
                "editor.active_line.background": "#F2629E10",
                "panel.background": "#19143d90",
                "title_bar.background": "#19143d90",
                "title_bar.inactive_background": "#19143d90",
                "status_bar.background": "#19143d90",
                "drop_target.background": "#19143d90",
                "elevated_surface.background": "#19143d",
                "toolbar.background": "#19143d90",
                "tab_bar.background": "#19143d90",
                "tab.inactive_background": "#19143d90",
                "tab.active_background": "#F2629E30",
                "scrollbar.track.background": "transparent",
                "scrollbar.track.border": "#19143d",
                "scrollbar.thumb.background": "#F2629E",
                "editor.gutter.background": "#19143d90",
                "terminal.background": "#19143d10",
                "terminal.foreground": "#c5c4ce",
                "terminal.dim_foreground": "#c5c4ce",
                "terminal.bright_foreground": "#c5c4ce",
                "terminal.ansi.black": "#19143d",
                "terminal.ansi.red": "#F2629E",
                "terminal.ansi.green": "#1BB3C8",
                "terminal.ansi.yellow": "#30A1C0",
                "terminal.ansi.blue": "#1BC4D0",
                "terminal.ansi.magenta": "#2ACBD6",
                "terminal.ansi.cyan": "#F88695",
                "terminal.ansi.white": "#c5c4ce",
                "terminal.ansi.bright_black": "#6a677e",
                "terminal.ansi.bright_red": "#F2629E",
                "terminal.ansi.bright_green": "#1BB3C8",
                "terminal.ansi.bright_yellow": "#30A1C0",
                "terminal.ansi.bright_blue": "#1BC4D0",
                "terminal.ansi.bright_magenta": "#2ACBD6",
                "terminal.ansi.bright_cyan": "#F88695",
                "terminal.ansi.bright_white": "#c5c4ce",
                "modified": "#F2629E",
                "syntax": {
                    "attribute": {
                        "color": "#F2629E"
                    },
                    "boolean": {
                      "color": "#1BB3C8"
                    },
                    "comment": {
                      "color": "#6a677e"
                    },
                    "comment.doc": {
                      "color": "#6a677e"
                    },
                    "constant": {
                      "color": "#1BB3C8"
                    },
                    "function": {
                      "color": "#F88695"
                    },
                    "keyword": {
                      "color": "#1BC4D0"
                    },
                    "number": {
                      "color": "#2ACBD6"
                    },
                    "operator": {
                      "color": "#1BC4D0"
                    },
                    "string": {
                      "color": "#F2629E"
                    },
                    "variable": {
                      "color": "#1BB3C8"
                    }
                },
                "players": [
                  {
                    "cursor": "#F2629E",
                    "background": "#19143d",
                    "selection": "#F2629E30"
                  }
                ]
            }
        }
    ]
}
