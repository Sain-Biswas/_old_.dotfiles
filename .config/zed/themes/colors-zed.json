{
    "$schema": "https://zed.dev/schema/themes/v0.1.0.json",
    "name": "pywal",
    "author": "pywal",
    "themes": [
        {
            "name": "pywal",
            "appearance": "dark",
            "style": {
                "background": "#0a0d1190",
                "editor.background": "#0a0d1190",
                "editor.foreground": "#c1c2c3",
                "text": "#c1c2c3",
                "text.muted": "#c1c2c370",
                "text.ignored": "#c1c2c340",
                "text.placeholder": "#c1c2c350",
                "ignored": "#c1c2c330",
                "element.hover": "#c1c2c330",
                "ghost_element.hover": "#56586E30",
                "ghost_element.selected": "#56586E30",
                "ghost_element.active": "#56586E60",
                "border": "#0a0d11",
                "editor.highlighted_line.background": "#56586E10",
                "editor.active_line.background": "#56586E10",
                "panel.background": "#0a0d1190",
                "title_bar.background": "#0a0d1190",
                "title_bar.inactive_background": "#0a0d1190",
                "status_bar.background": "#0a0d1190",
                "drop_target.background": "#0a0d1190",
                "elevated_surface.background": "#0a0d11",
                "toolbar.background": "#0a0d1190",
                "tab_bar.background": "#0a0d1190",
                "tab.inactive_background": "#0a0d1190",
                "tab.active_background": "#56586E30",
                "scrollbar.track.background": "transparent",
                "scrollbar.track.border": "#0a0d11",
                "scrollbar.thumb.background": "#56586E",
                "editor.gutter.background": "#0a0d1190",
                "terminal.background": "#0a0d1110",
                "terminal.foreground": "#c1c2c3",
                "terminal.dim_foreground": "#c1c2c3",
                "terminal.bright_foreground": "#c1c2c3",
                "terminal.ansi.black": "#0a0d11",
                "terminal.ansi.red": "#56586E",
                "terminal.ansi.green": "#A0566D",
                "terminal.ansi.yellow": "#AB837E",
                "terminal.ansi.blue": "#67708A",
                "terminal.ansi.magenta": "#9F738B",
                "terminal.ansi.cyan": "#78869C",
                "terminal.ansi.white": "#c1c2c3",
                "terminal.ansi.bright_black": "#575f6b",
                "terminal.ansi.bright_red": "#56586E",
                "terminal.ansi.bright_green": "#A0566D",
                "terminal.ansi.bright_yellow": "#AB837E",
                "terminal.ansi.bright_blue": "#67708A",
                "terminal.ansi.bright_magenta": "#9F738B",
                "terminal.ansi.bright_cyan": "#78869C",
                "terminal.ansi.bright_white": "#c1c2c3",
                "modified": "#56586E",
                "syntax": {
                    "attribute": {
                        "color": "#56586E"
                    },
                    "boolean": {
                      "color": "#A0566D"
                    },
                    "comment": {
                      "color": "#575f6b"
                    },
                    "comment.doc": {
                      "color": "#575f6b"
                    },
                    "constant": {
                      "color": "#A0566D"
                    },
                    "function": {
                      "color": "#78869C"
                    },
                    "keyword": {
                      "color": "#67708A"
                    },
                    "number": {
                      "color": "#9F738B"
                    },
                    "operator": {
                      "color": "#67708A"
                    },
                    "string": {
                      "color": "#56586E"
                    },
                    "variable": {
                      "color": "#A0566D"
                    }
                },
                "players": [
                  {
                    "cursor": "#56586E",
                    "background": "#0a0d11",
                    "selection": "#56586E30"
                  }
                ]
            }
        }
    ]
}
