                    },
                    "comment": {
                      "color": "#725e5f"
                    },
                    "comment.doc": {
                      "color": "#725e5f"
                    },
                    "constant": {
                      "color": "#DB768A"
                    },
                    "function": {
                      "color": "#E6B0CB"
                    },
                    "keyword": {
                      "color": "#D79CAD"
                    },
                    "number": {
                      "color": "#A4B3E7"
                    },
                    "operator": {
                      "color": "#D79CAD"
                    },
                    "string": {
                      "color": "#987491"
                    },
                    "variable": {
                      "color": "#DB768A"
                    }
                },
                "players": [
                  {
                    "cursor": "#987491",
                    "background": "#211012",
                    "selection": "#98749130"
                  }
                ]
